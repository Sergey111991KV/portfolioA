    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@taiga-ui/cdk": "^3.8.0",
    "@taiga-ui/core": "^3.8.0",
    "@taiga-ui/icons": "^3.8.0",
    "@taiga-ui/kit": "^3.8.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
