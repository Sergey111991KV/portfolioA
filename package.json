{
  "name": "portfolio-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build-deploy": "ng build --base-href \"https://sergey111991kv.github.io/portfolioA/\"",
    "deploy-gh": "npm run build-deploy && npx angular-cli-ghpages --dir=dist/portfolio-angular",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
